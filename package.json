{
  "name": "server-strategy",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "repository": "git@github.com:Noamiz/server-strategy.git",
  "author": "Noamiz <noamizrachi8@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "db:migrate": "prisma migrate dev --config prisma.config.ts",
    "db:seed": "prisma db seed --config prisma.config.ts"
  },
  "dependencies": {
    "@prisma/client": "^7.0.0",
    "@prisma/adapter-pg": "^7.0.0",
    "common-strategy": "git+ssh://git@github.com:Noamiz/common-strategy.git",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^4.0.12",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
